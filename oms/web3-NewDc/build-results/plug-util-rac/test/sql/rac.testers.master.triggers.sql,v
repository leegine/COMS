text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- simple_test_master

create trigger inv_simple_test_master_ins
  after insert on simple_test_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_master', 'ins', 0, :new.name );
  end;
/
show errors

create trigger inv_simple_test_master_upd
  before update on simple_test_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_master', 'upd', 0, :old.name );
  end;
/
show errors

create trigger inv_simple_test_master_del
  before delete on simple_test_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_master', 'del', 0, :old.name );
  end;
/
show errors

@
