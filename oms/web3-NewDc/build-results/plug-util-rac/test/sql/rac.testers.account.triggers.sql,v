text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- simple_test_account

create trigger inv_simple_test_account_ins
  after insert on simple_test_account
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_account', 'ins', :new.id, :new.id );
  end;
/
show errors

create trigger inv_simple_test_account_upd
  before update on simple_test_account
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_account', 'upd', :old.id, :old.id );
  end;
/
show errors

create trigger inv_simple_test_account_del
  before delete on simple_test_account
  for each row
  declare
  begin
    invalidation.on_mutate( 'simple_test_account', 'del', :old.id, :old.id );
  end;
/
show errors

@
