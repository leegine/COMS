@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- inform_ctrl_item_master

create trigger inv_inform_ctrl_item_maste_ins
  after insert on inform_ctrl_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'inform_ctrl_item_master', 'ins', 0, :new.institution_code||','||:new.branch_code||','||:new.inform_div||','||:new.item_symbol_name );
  end;
/
show errors

create trigger inv_inform_ctrl_item_maste_upd
  before update on inform_ctrl_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'inform_ctrl_item_master', 'upd', 0, :old.institution_code||','||:old.branch_code||','||:old.inform_div||','||:old.item_symbol_name );
  end;
/
show errors

create trigger inv_inform_ctrl_item_maste_del
  before delete on inform_ctrl_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'inform_ctrl_item_master', 'del', 0, :old.institution_code||','||:old.branch_code||','||:old.inform_div||','||:old.item_symbol_name );
  end;
/
show errors

