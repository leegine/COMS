text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- f_cash_balance

create trigger inv_f_cash_balance_ins
  after insert on f_cash_balance
  for each row
  declare
  begin
    invalidation.on_mutate( 'f_cash_balance', 'ins', :new.account_id, :new.feq_cash_balance_id );
  end;
/
show errors

create trigger inv_f_cash_balance_upd
  before update on f_cash_balance
  for each row
  declare
  begin
    invalidation.on_mutate( 'f_cash_balance', 'upd', :old.account_id, :old.feq_cash_balance_id );
  end;
/
show errors

create trigger inv_f_cash_balance_del
  before delete on f_cash_balance
  for each row
  declare
  begin
    invalidation.on_mutate( 'f_cash_balance', 'del', :old.account_id, :old.feq_cash_balance_id );
  end;
/
show errors

@
