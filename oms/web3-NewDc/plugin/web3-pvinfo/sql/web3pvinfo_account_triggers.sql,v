text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- browse_history

create trigger inv_browse_history_ins
  after insert on browse_history
  for each row
  declare
  begin
    invalidation.on_mutate( 'browse_history', 'ins', :new.account_id, :new.browse_history_id );
  end;
/
show errors

create trigger inv_browse_history_upd
  before update on browse_history
  for each row
  declare
  begin
    invalidation.on_mutate( 'browse_history', 'upd', :old.account_id, :old.browse_history_id );
  end;
/
show errors

create trigger inv_browse_history_del
  before delete on browse_history
  for each row
  declare
  begin
    invalidation.on_mutate( 'browse_history', 'del', :old.account_id, :old.browse_history_id );
  end;
/
show errors

@
