@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- commission_course_regist

create trigger inv_commission_course_regi_ins
  after insert on commission_course_regist
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_regist', 'ins', :new.account_id, :new.commission_course_regist_id );
  end;
/
show errors

create trigger inv_commission_course_regi_upd
  before update on commission_course_regist
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_regist', 'upd', :old.account_id, :old.commission_course_regist_id );
  end;
/
show errors

create trigger inv_commission_course_regi_del
  before delete on commission_course_regist
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_regist', 'del', :old.account_id, :old.commission_course_regist_id );
  end;
/
show errors

--------------------------------------------------
--- mobile_office_info_regist

