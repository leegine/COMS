@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- commission_course_master

create trigger inv_commission_course_mast_ins
  after insert on commission_course_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_master', 'ins', 0, :new.institution_code||','||:new.comm_product_code||','||:new.commission_course_div );
  end;
/
show errors

create trigger inv_commission_course_mast_upd
  before update on commission_course_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_master', 'upd', 0, :old.institution_code||','||:old.comm_product_code||','||:old.commission_course_div );
  end;
/
show errors

create trigger inv_commission_course_mast_del
  before delete on commission_course_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'commission_course_master', 'del', 0, :old.institution_code||','||:old.comm_product_code||','||:old.commission_course_div );
  end;
/
show errors

--------------------------------------------------
--- information_mail_request

create trigger inv_information_mail_reque_ins
  after insert on information_mail_request
  for each row
  declare
  begin
    invalidation.on_mutate( 'information_mail_request', 'ins', 0, :new.information_mail_request_id );
  end;
/
show errors

create trigger inv_information_mail_reque_upd
  before update on information_mail_request
  for each row
  declare
  begin
    invalidation.on_mutate( 'information_mail_request', 'upd', 0, :old.information_mail_request_id );
  end;
/
show errors

create trigger inv_information_mail_reque_del
  before delete on information_mail_request
  for each row
  declare
  begin
    invalidation.on_mutate( 'information_mail_request', 'del', 0, :old.information_mail_request_id );
  end;
/
show errors

@
