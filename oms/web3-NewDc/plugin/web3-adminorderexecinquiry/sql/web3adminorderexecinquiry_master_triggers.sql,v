@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- order_executed_count

create trigger inv_order_executed_count_ins
  after insert on order_executed_count
  for each row
  declare
  begin
    invalidation.on_mutate( 'order_executed_count', 'ins', 0, :new.institution_code||','||:new.branch_code||','||:new.record_div||','||:new.market_code||','||:new.product_div||','||:new.order_chanel||','||:new.order_root_div||','||:new.biz_date );
  end;
/
show errors

create trigger inv_order_executed_count_upd
  before update on order_executed_count
  for each row
  declare
  begin
    invalidation.on_mutate( 'order_executed_count', 'upd', 0, :old.institution_code||','||:old.branch_code||','||:old.record_div||','||:old.market_code||','||:old.product_div||','||:old.order_chanel||','||:old.order_root_div||','||:old.biz_date );
  end;
/
show errors

create trigger inv_order_executed_count_del
  before delete on order_executed_count
  for each row
  declare
  begin
    invalidation.on_mutate( 'order_executed_count', 'del', 0, :old.institution_code||','||:old.branch_code||','||:old.record_div||','||:old.market_code||','||:old.product_div||','||:old.order_chanel||','||:old.order_root_div||','||:old.biz_date );
  end;
/
show errors

@
