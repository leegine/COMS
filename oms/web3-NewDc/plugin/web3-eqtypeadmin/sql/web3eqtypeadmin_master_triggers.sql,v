text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- eqtype_product_condition

create trigger inv_eqtype_product_conditi_ins
  after insert on eqtype_product_condition
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_product_condition', 'ins', 0, :new.eqtype_product_condition_id );
  end;
/
show errors

create trigger inv_eqtype_product_conditi_upd
  before update on eqtype_product_condition
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_product_condition', 'upd', 0, :old.eqtype_product_condition_id );
  end;
/
show errors

create trigger inv_eqtype_product_conditi_del
  before delete on eqtype_product_condition
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_product_condition', 'del', 0, :old.eqtype_product_condition_id );
  end;
/
show errors

@
