@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- srv_regi_master

create trigger inv_srv_regi_master_ins
  after insert on srv_regi_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'srv_regi_master', 'ins', 0, :new.institution_code||','||:new.srv_div );
  end;
/
show errors

create trigger inv_srv_regi_master_upd
  before update on srv_regi_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'srv_regi_master', 'upd', 0, :old.institution_code||','||:old.srv_div );
  end;
/
show errors

create trigger inv_srv_regi_master_del
  before delete on srv_regi_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'srv_regi_master', 'del', 0, :old.institution_code||','||:old.srv_div );
  end;
/
show errors

