@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- eqtype_fin_transaction

create trigger inv_eqtype_fin_transaction_ins
  after insert on eqtype_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_fin_transaction', 'ins', :new.account_id, :new.fin_transaction_id );
  end;
/
show errors

create trigger inv_eqtype_fin_transaction_upd
  before update on eqtype_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_fin_transaction', 'upd', :old.account_id, :old.fin_transaction_id );
  end;
/
show errors

create trigger inv_eqtype_fin_transaction_del
  before delete on eqtype_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'eqtype_fin_transaction', 'del', :old.account_id, :old.fin_transaction_id );
  end;
/
show errors

