text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- mq_message_id_mappings

create trigger inv_mq_message_id_mappings_ins
  after insert on mq_message_id_mappings
  for each row
  declare
  begin
    invalidation.on_mutate( 'mq_message_id_mappings', 'ins', 0, :new.institution_code||','||:new.data_code );
  end;
/
show errors

create trigger inv_mq_message_id_mappings_upd
  before update on mq_message_id_mappings
  for each row
  declare
  begin
    invalidation.on_mutate( 'mq_message_id_mappings', 'upd', 0, :old.institution_code||','||:old.data_code );
  end;
/
show errors

create trigger inv_mq_message_id_mappings_del
  before delete on mq_message_id_mappings
  for each row
  declare
  begin
    invalidation.on_mutate( 'mq_message_id_mappings', 'del', 0, :old.institution_code||','||:old.data_code );
  end;
/
show errors

@
