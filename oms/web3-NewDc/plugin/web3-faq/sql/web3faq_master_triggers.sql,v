text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- faq_number

create trigger inv_faq_number_ins
  after insert on faq_number
  for each row
  declare
  begin
    invalidation.on_mutate( 'faq_number', 'ins', 0, :new.institution_code );
  end;
/
show errors

create trigger inv_faq_number_upd
  before update on faq_number
  for each row
  declare
  begin
    invalidation.on_mutate( 'faq_number', 'upd', 0, :old.institution_code );
  end;
/
show errors

create trigger inv_faq_number_del
  before delete on faq_number
  for each row
  declare
  begin
    invalidation.on_mutate( 'faq_number', 'del', 0, :old.institution_code );
  end;
/
show errors

@
