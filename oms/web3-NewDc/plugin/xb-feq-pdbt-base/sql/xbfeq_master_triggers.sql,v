text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- feq_product

create trigger inv_feq_product_ins
  after insert on feq_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_product', 'ins', 0, :new.product_id );
  end;
/
show errors

create trigger inv_feq_product_upd
  before update on feq_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_product', 'upd', 0, :old.product_id );
  end;
/
show errors

create trigger inv_feq_product_del
  before delete on feq_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_product', 'del', 0, :old.product_id );
  end;
/
show errors

--------------------------------------------------
--- feq_traded_product

create trigger inv_feq_traded_product_ins
  after insert on feq_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product', 'ins', 0, :new.traded_product_id );
  end;
/
show errors

create trigger inv_feq_traded_product_upd
  before update on feq_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product', 'upd', 0, :old.traded_product_id );
  end;
/
show errors

create trigger inv_feq_traded_product_del
  before delete on feq_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product', 'del', 0, :old.traded_product_id );
  end;
/
show errors

--------------------------------------------------
--- feq_traded_product_updq

create trigger inv_feq_traded_product_upd_ins
  after insert on feq_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product_updq', 'ins', 0, :new.traded_product_id||','||:new.valid_for_biz_date );
  end;
/
show errors

create trigger inv_feq_traded_product_upd_upd
  before update on feq_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product_updq', 'upd', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

create trigger inv_feq_traded_product_upd_del
  before delete on feq_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'feq_traded_product_updq', 'del', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

