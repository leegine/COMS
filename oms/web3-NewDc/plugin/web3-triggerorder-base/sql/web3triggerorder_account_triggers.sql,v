@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- rsv_eq_order_unit

create trigger inv_rsv_eq_order_unit_ins
  after insert on rsv_eq_order_unit
  for each row
  declare
  begin
    invalidation.on_mutate( 'rsv_eq_order_unit', 'ins', :new.account_id, :new.order_id );
  end;
/
show errors

create trigger inv_rsv_eq_order_unit_upd
  before update on rsv_eq_order_unit
  for each row
  declare
  begin
    invalidation.on_mutate( 'rsv_eq_order_unit', 'upd', :old.account_id, :old.order_id );
  end;
/
show errors

create trigger inv_rsv_eq_order_unit_del
  before delete on rsv_eq_order_unit
  for each row
  declare
  begin
    invalidation.on_mutate( 'rsv_eq_order_unit', 'del', :old.account_id, :old.order_id );
  end;
/
show errors

