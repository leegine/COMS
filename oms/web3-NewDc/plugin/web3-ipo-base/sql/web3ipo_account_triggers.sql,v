@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- ipo_order

create trigger inv_ipo_order_ins
  after insert on ipo_order
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_order', 'ins', :new.account_id, :new.ipo_order_id );
  end;
/
show errors

create trigger inv_ipo_order_upd
  before update on ipo_order
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_order', 'upd', :old.account_id, :old.ipo_order_id );
  end;
/
show errors

create trigger inv_ipo_order_del
  before delete on ipo_order
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_order', 'del', :old.account_id, :old.ipo_order_id );
  end;
/
show errors

--------------------------------------------------
--- ipo_bookbuilding_order_action

create trigger inv_ipo_bookbuilding_order_ins
  after insert on ipo_bookbuilding_order_action
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_bookbuilding_order_action', 'ins', :new.account_id, :new.bookbuilding_order_action_id );
  end;
/
show errors

create trigger inv_ipo_bookbuilding_order_upd
  before update on ipo_bookbuilding_order_action
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_bookbuilding_order_action', 'upd', :old.account_id, :old.bookbuilding_order_action_id );
  end;
/
show errors

create trigger inv_ipo_bookbuilding_order_del
  before delete on ipo_bookbuilding_order_action
  for each row
  declare
  begin
    invalidation.on_mutate( 'ipo_bookbuilding_order_action', 'del', :old.account_id, :old.bookbuilding_order_action_id );
  end;
/
show errors

@
