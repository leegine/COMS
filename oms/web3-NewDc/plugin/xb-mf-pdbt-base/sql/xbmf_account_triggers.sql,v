text
@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- mutual_fund_fin_transaction

create trigger inv_mutual_fund_fin_transa_ins
  after insert on mutual_fund_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_fin_transaction', 'ins', :new.account_id, :new.fin_transaction_id );
  end;
/
show errors

create trigger inv_mutual_fund_fin_transa_upd
  before update on mutual_fund_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_fin_transaction', 'upd', :old.account_id, :old.fin_transaction_id );
  end;
/
show errors

create trigger inv_mutual_fund_fin_transa_del
  before delete on mutual_fund_fin_transaction
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_fin_transaction', 'del', :old.account_id, :old.fin_transaction_id );
  end;
/
show errors

