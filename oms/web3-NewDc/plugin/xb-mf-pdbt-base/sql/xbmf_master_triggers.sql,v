@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- mutual_fund_traded_product

create trigger inv_mutual_fund_traded_pro_ins
  after insert on mutual_fund_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_traded_product', 'ins', 0, :new.traded_product_id );
  end;
/
show errors

create trigger inv_mutual_fund_traded_pro_upd
  before update on mutual_fund_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_traded_product', 'upd', 0, :old.traded_product_id );
  end;
/
show errors

create trigger inv_mutual_fund_traded_pro_del
  before delete on mutual_fund_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'mutual_fund_traded_product', 'del', 0, :old.traded_product_id );
  end;
/
show errors

--------------------------------------------------
--- mf_traded_product_updq

create trigger inv_mf_traded_product_updq_ins
  after insert on mf_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'mf_traded_product_updq', 'ins', 0, :new.traded_product_id||','||:new.valid_for_biz_date );
  end;
/
show errors

create trigger inv_mf_traded_product_updq_upd
  before update on mf_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'mf_traded_product_updq', 'upd', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

create trigger inv_mf_traded_product_updq_del
  before delete on mf_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'mf_traded_product_updq', 'del', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

