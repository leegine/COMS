);

--Index
CREATE UNIQUE INDEX XPKacc_open_invalid_values ON acc_open_invalid_values
(
    institution_code,
    acc_open_request_number
);

--Primary Key
ALTER TABLE acc_open_invalid_values
        ADD  (PRIMARY KEY (institution_code, acc_open_request_number));

