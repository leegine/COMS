);

--Primary Key
ALTER TABLE exp_account_open
        ADD  (PRIMARY KEY (institution_code, acc_open_request_number));


--Table: 口座開設項目マスタ
CREATE TABLE acc_open_item_master (
    institution_code            VARCHAR2(3)         NOT NULL,
    branch_code                 VARCHAR2(3)         NOT NULL,
    account_div                 VARCHAR2(1)         NOT NULL,
    validate_type               VARCHAR2(1)         NOT NULL,
    item_symbol_name            VARCHAR2(30)        NOT NULL,
    item_name                   VARCHAR2(50)            NULL,
    necessary_flag              NUMBER(1)           DEFAULT 0       NOT NULL,
    item_min                    NUMBER(6)               NULL,
    item_max                    NUMBER(6)               NULL,
    item_check_mode             VARCHAR2(2)             NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_item_master ON acc_open_item_master
(
    institution_code,
    branch_code,
    account_div,
    validate_type,
    item_symbol_name
);

--Primary Key
ALTER TABLE acc_open_item_master
        ADD  (PRIMARY KEY (institution_code, branch_code, account_div, validate_type, item_symbol_name));


--Table: 口座開設項目属性
CREATE TABLE acc_open_item_attribute (
    institution_code            VARCHAR2(3)         NOT NULL,
    branch_code                 VARCHAR2(3)         NOT NULL,
    account_div                 VARCHAR2(1)         NOT NULL,
    item_symbol_name            VARCHAR2(30)        NOT NULL,
    attribute_name              VARCHAR2(64)        NOT NULL,
    attribute_value             VARCHAR2(4)         NOT NULL,
    range_from                  DECIMAL(18,6)           NULL,
    range_to                    DECIMAL(18,6)           NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_item_attribute ON acc_open_item_attribute
(
    institution_code,
    branch_code,
    account_div,
    item_symbol_name,
    attribute_value
);

--Primary Key
ALTER TABLE acc_open_item_attribute
        ADD  (PRIMARY KEY (institution_code, branch_code, account_div, item_symbol_name, attribute_value));


--Table: 口座開設申込ＤＬフォームマスタ
CREATE TABLE acc_open_dl_form_master (
    institution_code            VARCHAR2(3)         NOT NULL,
    account_div                 VARCHAR2(1)         NOT NULL,
    column_number               NUMBER(6)           NOT NULL,
    column_label                VARCHAR2(64)            NULL,
    column_type                 NUMBER(6)           NOT NULL,
    date_format                 VARCHAR2(30)            NULL,
    input_item_symbol_name      VARCHAR2(30)        NOT NULL,
    cat_delimitter              VARCHAR2(1)             NULL,
    section_number              NUMBER(6)               NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_dl_form_master ON acc_open_dl_form_master
(
    institution_code,
    account_div,
    column_number
);

--Primary Key
ALTER TABLE acc_open_dl_form_master
        ADD  (PRIMARY KEY (institution_code, account_div, column_number));


--Table: 口座開設不備テーブル
