    edit_way_div                VARCHAR2(1)         NOT NULL,
    fixed_value                 VARCHAR2(128)           NULL,
    input_item_symbol_name1     VARCHAR2(30)            NULL,
    input_item_symbol_name2     VARCHAR2(30)            NULL,
    input_item_symbol_name3     VARCHAR2(30)            NULL,
    cat_delimitter              VARCHAR2(4)             NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_voucher_item ON acc_open_voucher_item
(
