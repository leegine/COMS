--Table: 口座開設識別コード
CREATE TABLE acc_open_request_number (
    institution_code                VARCHAR2(3)         NOT NULL,
    last_acc_open_request_number    VARCHAR2(13)        NOT NULL,
    created_timestamp               DATE                NOT NULL,
    last_updated_timestamp          DATE                NOT NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_request_number ON acc_open_request_number
(
    institution_code
);

--Primary Key
ALTER TABLE acc_open_request_number
        ADD  (PRIMARY KEY (institution_code));

--Table: Ｙ客マスタ
CREATE TABLE y_customer (
    y_customer_id              NUMBER(18)          NOT NULL,
    control_branch_code        VARCHAR2(3)         NOT NULL,
    operation_div              VARCHAR2(2)         NOT NULL,
    control_number             NUMBER(7)           NOT NULL,
    institution_code           VARCHAR2(3)         NOT NULL,
    era_born                   VARCHAR2(1)         NULL,
    born_date                  VARCHAR2(6)         NULL,
    name_kana                  VARCHAR2(42)        NOT NULL,
    name                       VARCHAR2(42)        NOT NULL,
    account_name_kana          VARCHAR2(42)        NULL,
    account_name               VARCHAR2(42)        NULL,
    address_kana               VARCHAR2(104)       NULL,
    address                    VARCHAR2(104)       NULL,
    zip_code                   VARCHAR2(7)         NULL,
    telephone                  VARCHAR2(14)        NULL,
    office_kana                VARCHAR2(52)        NULL,
    office                     VARCHAR2(52)        NULL,
    post_kana                  VARCHAR2(102)       NULL,
    post                       VARCHAR2(102)       NULL,
    comment_kana               VARCHAR2(52)        NULL,
    comment_kanji              VARCHAR2(52)        NULL,
    sonar_created_branch       VARCHAR2(3)         NULL,
    sonar_created_date         VARCHAR2(6)         NULL,
    sonar_updated_branch       VARCHAR2(3)         NULL,
    sonar_updated_date         VARCHAR2(6)         NULL,
    disclosure_div             VARCHAR2(1)         NULL,
    sonar_check_div            VARCHAR2(1)         NULL,
    created_timestamp          DATE                NOT NULL,
    last_updated_timestamp     DATE                NOT NULL
);

--Index
CREATE UNIQUE INDEX XPKy_customer ON y_customer
(
    y_customer_id
);

CREATE UNIQUE INDEX XAKy_custmer ON y_customer
(
    control_branch_code   ,
    operation_div         ,
    control_number        ,
    institution_code
);

CREATE INDEX XIEy_custmer ON y_customer
(
    institution_code      ,
    era_born              ,
    born_date             ,
    name_kana             ,
    name
);

--Primary Key
ALTER TABLE y_customer
        ADD  (PRIMARY KEY (y_customer_id));


