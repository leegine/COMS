    gp_tax_div                  VARCHAR2(1)              NULL,
    gp_taxfree_limit            VARCHAR2(3)              NULL,
    gp_special_taxfree_limit    VARCHAR2(3)              NULL,
    gp_subscr_summary           VARCHAR2(1)              NULL,
    gp_product_code             VARCHAR2(1)              NULL,
    gp_mortgage_customer        VARCHAR2(1)              NULL,
    gp_mix_customer             VARCHAR2(1)              NULL,
    gp_contract                 VARCHAR2(1)              NULL,
    stk_voucher_div             VARCHAR2(1)              NULL,
    stk_taxation_tran_div       VARCHAR2(1)              NULL,
    stk_address_line_kana       VARCHAR2(70)             NULL,
    stk_transfer_div            VARCHAR2(1)              NULL,
    stk_fin_institution_code    VARCHAR2(4)              NULL,
    stk_fin_branch_code         VARCHAR2(3)              NULL,
    stk_fin_save_div            VARCHAR2(1)              NULL,
    stk_fin_account_no          VARCHAR2(7)              NULL,
    brokerage_trader_code       VARCHAR2(5)              NULL,
    ext_item_div11              VARCHAR2(10)             NULL,
    ext_item_div12              VARCHAR2(10)             NULL,
    ext_item_div13              VARCHAR2(10)             NULL,
    ext_item_div14              VARCHAR2(10)             NULL,
    ext_item_div15              VARCHAR2(10)             NULL,
    foreign_account_no          VARCHAR2(20)             NULL,
    foreign_account_name        VARCHAR2(60)             NULL,
    foreign_account_name_eng    VARCHAR2(30)             NULL,
    foreign_save_div            VARCHAR2(1)              NULL,
    delete_flag                 NUMBER(1)                NULL,
    delete_timestamp            DATE                     NULL,
    print_flag                  VARCHAR2(1)              NULL,
    receipt_flag                NUMBER(1)                NULL,
    agreement_flag              NUMBER(1)                NULL,
    foreign_flag                NUMBER(1)                NULL,
    agency_acc_name_kana1       VARCHAR2(60)             NULL,
    agency_acc_name_kana2       VARCHAR2(60)             NULL,
    agency_acc_name1            VARCHAR2(60)             NULL,
    agency_acc_name2            VARCHAR2(60)             NULL,
    agency_address_line1        VARCHAR2(48)             NULL,
    agency_address_line2        VARCHAR2(48)             NULL,
    agency_rep_post             VARCHAR2(40)             NULL,
    agency_rep_name_kana1       VARCHAR2(60)             NULL,
    agency_rep_name_kana2       VARCHAR2(60)             NULL,
    agency_rep_name1            VARCHAR2(60)             NULL,
    agency_rep_name2            VARCHAR2(60)             NULL,
    status                      VARCHAR2(1)              NOT NULL,
    error_info                  VARCHAR2(200)            NULL
);

--Index
CREATE UNIQUE INDEX XPKexp_account_open_temp ON exp_account_open_temp
(
    institution_code,
    acc_open_request_number
);

CREATE INDEX XIE1exp_account_open_temp ON exp_account_open_temp
(
    institution_code,
    status
);

--Primary Key
ALTER TABLE exp_account_open_temp
        ADD  (PRIMARY KEY (institution_code, acc_open_request_number));

-------------------------------------------------------------------------------
--FOREIGN KEY
-------------------------------------------------------------------------------

--1. Table名: 口座開設見込客
ALTER TABLE exp_account_open
        ADD  (FOREIGN KEY (institution_id) REFERENCES institution);

ALTER TABLE exp_account_open
       ADD  ( FOREIGN KEY (branch_id) REFERENCES branch) ;

