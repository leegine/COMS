    comment7                    VARCHAR2(50)            NULL,
    comp_flag7                  NUMBER(1)               NULL,
    item_name8                  VARCHAR2(30)            NULL,
    comment8                    VARCHAR2(50)            NULL,
    comp_flag8                  NUMBER(1)               NULL,
