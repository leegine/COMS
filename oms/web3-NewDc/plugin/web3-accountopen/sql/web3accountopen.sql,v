--Table: 暗証番号伝票(G5511)テーブル
CREATE TABLE password_voucher (
    order_request_number        VARCHAR2(9)         NOT NULL,
    request_code                VARCHAR2(5)         NOT NULL,
    institution_code            VARCHAR2(3)         NOT NULL,
    branch_code                 VARCHAR2(3)         NOT NULL,
    account_code                VARCHAR2(7)         NOT NULL,
    trader_code                 VARCHAR2(5)             NULL,
    acc_open_request_number     VARCHAR2(13)        NOT NULL,
    serial_no                   VARCHAR2(3)         DEFAULT 0       NOT NULL,
    regist_delete_div           VARCHAR2(1)             NULL,
    data_class                  VARCHAR2(2)             NULL,
    voucher_name                VARCHAR2(4)             NULL,
    regist_date                 VARCHAR2(6)             NULL,
    password                    VARCHAR2(48)            NULL,
    pub_reason_div              VARCHAR2(1)             NULL,
    ref_number                  VARCHAR2(5)             NULL,
    modified_name               VARCHAR2(19)            NULL,
    card_type                   VARCHAR2(1)             NULL,
    online_batch_div            VARCHAR2(1)             NULL,
    seq_no                      VARCHAR2(4)             NULL,
    status                      VARCHAR2(1)             NULL,
    send_timestamp              DATE                    NULL,
    created_timestamp           DATE                NOT NULL,
    last_updated_timestamp      DATE                NOT NULL
);

--Index
CREATE UNIQUE INDEX XPKpassword_voucher ON password_voucher
(
    order_request_number,
    request_code,
    institution_code,
    branch_code,
    account_code
);

--Primary Key
ALTER TABLE password_voucher
        ADD  (PRIMARY KEY (order_request_number, request_code, institution_code, branch_code, account_code));


--Table: 口座開設伝票マスタ
CREATE TABLE acc_open_voucher_master (
    institution_code            VARCHAR2(3)         NOT NULL,
    branch_code                 VARCHAR2(3)         NOT NULL,
    account_div                 VARCHAR2(1)         NOT NULL,
    request_code                VARCHAR2(5)         NOT NULL,
    serial_no                   VARCHAR2(3)         NOT NULL,
    create_voucher_column       VARCHAR2(30)            NULL,
    create_voucher_value        VARCHAR2(4)             NULL
);

--Index
CREATE UNIQUE INDEX XPKacc_open_voucher_master ON acc_open_voucher_master
(
    institution_code,
    branch_code,
    account_div,
    request_code,
    serial_no
);

--Primary Key
ALTER TABLE acc_open_voucher_master
        ADD  (PRIMARY KEY (institution_code, branch_code, account_div, request_code, serial_no));


--Table: 口座開設伝票項目マスタ
CREATE TABLE acc_open_voucher_item (
