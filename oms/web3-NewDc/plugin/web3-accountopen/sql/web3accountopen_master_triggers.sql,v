@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- acc_open_item_master

create trigger inv_acc_open_item_master_ins
  after insert on acc_open_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'acc_open_item_master', 'ins', 0, :new.institution_code||','||:new.branch_code||','||:new.account_div||','||:new.validate_type||','||:new.item_symbol_name );
  end;
/
show errors

create trigger inv_acc_open_item_master_upd
  before update on acc_open_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'acc_open_item_master', 'upd', 0, :old.institution_code||','||:old.branch_code||','||:old.account_div||','||:old.validate_type||','||:old.item_symbol_name );
  end;
/
show errors

create trigger inv_acc_open_item_master_del
  before delete on acc_open_item_master
  for each row
  declare
  begin
    invalidation.on_mutate( 'acc_open_item_master', 'del', 0, :old.institution_code||','||:old.branch_code||','||:old.account_div||','||:old.validate_type||','||:old.item_symbol_name );
  end;
/
show errors

