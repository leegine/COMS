  before delete on ruito_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_product', 'del', 0, :old.product_id );
  end;
/
show errors

--------------------------------------------------
--- ruito_traded_product_updq

create trigger inv_ruito_traded_product_u_ins
  after insert on ruito_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product_updq', 'ins', 0, :new.traded_product_id||','||:new.valid_for_biz_date );
  end;
/
show errors

create trigger inv_ruito_traded_product_u_upd
  before update on ruito_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product_updq', 'upd', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

create trigger inv_ruito_traded_product_u_del
  before delete on ruito_traded_product_updq
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product_updq', 'del', 0, :old.traded_product_id||','||:old.valid_for_biz_date );
  end;
/
show errors

