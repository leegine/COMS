@------------------------------------------------------------------
-- Autogenerated per-table triggers for mutators
------------------------------------------------------------------

--------------------------------------------------
--- ruito_traded_product

create trigger inv_ruito_traded_product_ins
  after insert on ruito_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product', 'ins', 0, :new.traded_product_id );
  end;
/
show errors

create trigger inv_ruito_traded_product_upd
  before update on ruito_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product', 'upd', 0, :old.traded_product_id );
  end;
/
show errors

create trigger inv_ruito_traded_product_del
  before delete on ruito_traded_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_traded_product', 'del', 0, :old.traded_product_id );
  end;
/
show errors

--------------------------------------------------
--- ruito_product

create trigger inv_ruito_product_ins
  after insert on ruito_product
  for each row
  declare
  begin
    invalidation.on_mutate( 'ruito_product', 'ins', 0, :new.product_id );
  end;
/
show errors

